<h1>Список каталогов</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Locality</th>
      <th>Addr</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Siteurl</th>
      <th>Description</th>
      <th>Biztype</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @catalogs.each do |catalog| %>
      <tr>
        <td><%= catalog.name %></td>
        <td><%= catalog.locality %></td>
        <td><%= catalog.addr %></td>
        <td><%= catalog.phone %></td>
        <td><%= catalog.email %></td>
        <td><%= catalog.siteurl %></td>
        <td><%= catalog.description %></td>
        <td><%= catalog.biztype %></td>
        <td><%= link_to 'Просмотреть', catalog %></td>
        <td><%= link_to 'Редактировать', edit_catalog_path(catalog) %></td>
        <td><%= link_to 'Удалить', catalog_destroy_path(catalog) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Создать каталог', new_catalog_path %>
